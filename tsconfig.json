{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "preserveConstEnums": true,
    "lib": ["webworker", "dom", "es6", "es2022"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "rootDir": "../",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
